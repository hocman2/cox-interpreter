class Zero {
  constructor() {
    this.myValue = 3;
  }
}

class A < Zero {
  constructor() {
    super.constructor();
    print "Hello from A";
    print this;
  }
}

class B < A {
  constructor() {
    print this.myValue;
    super.constructor();
    print "Hello from B";
    print super;
    print this;
    print super.myValue;
    print this.myValue;
  }
}

B();
