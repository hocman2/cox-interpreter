class A {
  constructor(message) {
    this.message = message;
  }

  printMsg() {
    print this.message;
  }
}

class B {
  constructor(message) {
    this.message = message;
  }

  getAInstance() {
    return A(this.message);
  }
}

for (var i = 0; i < 10; i = i + 1) {
  B(i).getAInstance().printMsg();
}
